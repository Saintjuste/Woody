			<posy>535</posy>
			<font>aller_4_20</font>
			<textcolor>greym</textcolor>
			<label>$INFO[Player.FinishTime]</label>
			<align>right</align>
		</control>
	</control>



	<control type="group">
		<posx>50</posx>
		<posy>300</posy>
