			<textcolor>greym</textcolor>
			<label>x2</label>
			<align>center</align>
			<visible>player.forwarding2x | player.rewinding2x</visible>
		</control>
 		<control type="label">
			<posx>-166</posx>
			<posy>15</posy>
			<font>aller_4_25</font>
			<textcolor>greym</textcolor>
			<label>x4</label>
			<align>center</align>
