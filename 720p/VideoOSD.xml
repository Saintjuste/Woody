			<textcolor>greym</textcolor>
			<label>x32</label>
			<align>center</align>
			<visible>player.forwarding32x | player.rewinding32x</visible>
		</control>
	</control>

	<!--Progress & Duration/Position-->
	<control type="group">
		<control type="label" id="402">
    			<description>current seek position</description>
			<posx>75</posx>
			<posy>568</posy>
			<width>300</width>
			<font>aller_3_18</font>
			<textcolor>greym</textcolor>
